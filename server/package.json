{
  "name": "voluntariado-server",
  "version": "1.0.0",
  "description": "voluntariado-server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node .",
    "dev": "nodemon",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run clean && npm run build",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^6.0.1",
    "@loopback/authentication-jwt": "^0.6.0",
    "@loopback/authorization": "^0.6.6",
    "@loopback/boot": "^2.5.0",
    "@loopback/core": "^2.9.4",
    "@loopback/repository": "^2.11.1",
    "@loopback/rest": "^6.1.0",
    "@loopback/rest-crud": "^0.8.12",
    "@loopback/rest-explorer": "^2.2.9",
    "@loopback/security": "^0.2.18",
    "@loopback/service-proxy": "^2.3.7",
    "geolib": "^3.3.1",
    "lodash": "^4.17.20",
    "loopback-connector-mongodb": "^5.3.0",
    "loopback-connector-rest": "^3.7.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.1",
    "@loopback/eslint-config": "^9.0.1",
    "@loopback/testlab": "^3.2.3",
    "@types/lodash": "^4.14.158",
    "@types/node": "^10.17.28",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.2"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm start"
  }
}
